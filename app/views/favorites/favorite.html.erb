<%= render 'users/list', user: @user %>
	<div class="col-lg-9">
	   	<div class="title-headline">My Favorite Artists</div>
	   	<% @user.favorites.each do |favo| %>
	   		<div class="cd-box">
	   			<div class="artist_index">
	   				<%= link_to artist_path(favo.artist.id) do %>
	   			<%= attachment_image_tag favo.artist,:image %>
	   			<% end %>
	   			</div>
	   			<%= link_to artist_path(favo.artist.id) do %>
				<%= favo.artist.artist_name %>
				<% end %>
			</div>
		<% end %>
	</div>
</div>