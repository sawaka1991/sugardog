    <%= render 'users/list', user: current_user %>
	<div class="col-lg-9">
	    <div class="main-menu">
			<div class="container">
				<div class="col-lg-4 col-lg-offset-2">
						<div class="title-headline">Review</div>
						<table class="table">
							<thead>
								<tr>
									<td>
										<%= attachment_image_tag @cd,:image %>
									</td>
								</tr>
								<tr>
									<td>
										<%= @cd.album_name %><br>
										<%= @cd.artist.artist_name %>
									</td>
								</tr>
							</thead>
		            	</table>
		            	<div class="form-horizontal">
						  <div class="form-group">
						  <%= form_for(@review, url: cd_reviews_path(@cd.id)) do |f| %>
						  <div class="review_text">総合評価</div>
						     <img src="/assets/star-on.png">
  						  <%= f.number_field :star, :min => 1, :max => 5 %><br>
						    <label for="comment">Comment:</label>
  						  <%= f.text_area :comment, class: "form-control" %><br>
  						  <%= f.submit "投稿", class: "btn btn-success", style: "padding-bottom: 5px"%>
						  <% end %>
						  </div>
						</div>
				</div>
			</div>
	    </div>
	</div>
